
<div class='container'>
  <div class='row justify-content-center'>
    <div class='col-10'>
    <h1> Log in </h1>
      <%= form_with(scope: :session, url: login_path, local: true) do |f| %>
        <p> 
          <%= f.label :username %><br/> 
          <%= f.text_field :username %>
        </p>
        <p>
          <%= f.submit :LOGIN %> 
        </p>
      <% end %>
    </div>
  </div>
</div>